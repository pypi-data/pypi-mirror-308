{
    "name": "neurosift-jp",
    "version": "0.1.3",
    "description": "Neurosift Jupyter extension",
    "keywords": [
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension"
    ],
    "homepage": "https://github.com/magland/neurosift-jp",
    "bugs": {
        "url": "https://github.com/magland/neurosift-jp/issues"
    },
    "license": "BSD-3-Clause",
    "author": {
        "name": "Jeremy Magland",
        "email": "jmagland@flatironinstitute.org"
    },
    "files": [
        "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf,css}",
        "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
        "src/**/*.{ts,tsx,css}"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "style": "style/index.css",
    "repository": {
        "type": "git",
        "url": "https://github.com/magland/neurosift-jp.git"
    },
    "scripts": {
        "copy:css": "find src -name '*.css' -exec sh -c 'mkdir -p lib/$(dirname {}|cut -c5-) && cp {} lib/$(dirname {}|cut -c5-)/$(basename {})' \\;",
        "build": "jlpm build:lib && jlpm copy:css && jlpm build:labextension:dev",
        "build:prod": "jlpm clean && jlpm build:lib:prod && jlpm copy:css && jlpm build:labextension",
        "build:labextension": "jupyter labextension build .",
        "build:labextension:dev": "jupyter labextension build --development True .",
        "build:lib": "tsc --sourceMap",
        "build:lib:prod": "tsc",
        "clean": "jlpm clean:lib",
        "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
        "clean:lintcache": "rimraf .eslintcache .stylelintcache",
        "clean:labextension": "rimraf neurosift_jp/labextension neurosift_jp/_version.py",
        "clean:all": "jlpm clean:lib && jlpm clean:labextension && jlpm clean:lintcache",
        "eslint": "jlpm eslint:check --fix",
        "eslint:check": "eslint . --cache --ext .ts,.tsx",
        "install:extension": "jlpm build",
        "lint": "jlpm stylelint && jlpm prettier && jlpm eslint",
        "lint:check": "jlpm stylelint:check && jlpm prettier:check && jlpm eslint:check",
        "prettier": "jlpm prettier:base --write --list-different",
        "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
        "prettier:check": "jlpm prettier:base --check",
        "stylelint": "jlpm stylelint:check --fix",
        "stylelint:check": "stylelint --cache \"style/**/*.css\"",
        "watch": "run-p watch:src watch:labextension",
        "watch:src": "tsc -w --sourceMap",
        "watch:labextension": "jupyter labextension watch ."
    },
    "dependencies": {
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@fi-sci/misc": "^0.0.10",
        "@fi-sci/modal-window": "^0.0.2",
        "@fortawesome/fontawesome-svg-core": "^6.6.0",
        "@fortawesome/free-solid-svg-icons": "^6.6.0",
        "@fortawesome/react-fontawesome": "^0.2.2",
        "@jupyter-widgets/base": "^6.0.10",
        "@jupyterlab/application": "^4.0.0",
        "@jupyterlab/launcher": "^4.3.0",
        "@jupyterlab/notebook": "^4.3.0",
        "@jupyterlab/services": "^7.3.0",
        "@mui/icons-material": "^5.15.17",
        "@mui/material": "^5.15.17",
        "@vercel/analytics": "^1.3.1",
        "github-markdown-css": "^5.5.0",
        "jsonpath": "^1.1.1",
        "mathjs": "^13.0.3",
        "numcodecs": "^0.3.1",
        "pako": "^2.1.0",
        "plotly.js": "^2.34.0",
        "pubnub": "^8.2.7",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-draggable": "^4.4.6",
        "react-icons": "^5.2.1",
        "react-intersection-observer": "^9.13.0",
        "react-markdown": "^8",
        "react-plotly.js": "^2.6.0",
        "react-router-dom": "^6.17.0",
        "react-syntax-highlighter": "^15.5.0",
        "rehype-mathjax": "^4.0.2",
        "rehype-raw": "^6",
        "remark-gfm": "^3.0.1",
        "remark-math": "^5.1.1"
    },
    "devDependencies": {
        "@jupyterlab/builder": "^4.0.0",
        "@types/chroma-js": "^2.4.4",
        "@types/json-schema": "^7.0.11",
        "@types/jsonpath": "^0.2.4",
        "@types/node": "^20.12.11",
        "@types/pako": "^2.0.3",
        "@types/pubnub": "^7.4.2",
        "@types/react": "^18.2.15",
        "@types/react-addons-linked-state-mixin": "^0.14.22",
        "@types/react-dom": "^18.2.7",
        "@types/react-plotly.js": "^2.6.3",
        "@types/react-syntax-highlighter": "^15.5.13",
        "@typescript-eslint/eslint-plugin": "^6.1.0",
        "@typescript-eslint/parser": "^6.1.0",
        "css-loader": "^6.7.1",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^5.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.0.0",
        "rimraf": "^5.0.1",
        "source-map-loader": "^1.0.2",
        "style-loader": "^3.3.1",
        "stylelint": "^15.10.1",
        "stylelint-config-recommended": "^13.0.0",
        "stylelint-config-standard": "^34.0.0",
        "stylelint-csstree-validator": "^3.0.0",
        "stylelint-prettier": "^4.0.0",
        "typescript": "~5.0.2",
        "yjs": "^13.5.0"
    },
    "sideEffects": [
        "style/*.css",
        "style/index.js"
    ],
    "styleModule": "style/index.js",
    "publishConfig": {
        "access": "public"
    },
    "jupyterlab": {
        "extension": true,
        "outputDir": "neurosift_jp/labextension",
        "sharedPackages": {
            "@jupyter-widgets/base": {
                "bundled": false,
                "singleton": true
            }
        }
    },
    "eslintIgnore": [
        "node_modules",
        "dist",
        "coverage",
        "**/*.d.ts"
    ],
    "prettier": {
        "tabWidth": 2,
        "semi": true,
        "experimentalTernaries": false,
        "bracketSameLine": false,
        "arrowParens": "always"
    }
}
